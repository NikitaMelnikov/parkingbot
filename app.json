{
  "name": "Parking bot",
  "description": "Parking bot for slack channel",
  "success_url": "/",
  "env": {
    "HEROKU_APP_NAME": {
      "required": true
    },
    "STARBOT_COMMAND_TOKEN": {
      "description": "Slash command token, for the bot command endpoint",
      "required": true
    },
    "SLACK_TOKEN": {
      "description": "Slack bot RTM API token",
      "required": false
    },
    "NODE_ENV": "staging"
  },
  "image": "heroku/nodejs"
}
